	<parameter name="desde" class="java.util.Date"/>
	<parameter name="hasta" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT
     C.FECHA_MOVIMIENTO ,
     SUM(C.ENTRADA) AS ENTRADA ,
     SUM(C.SALIDA) AS SALIDA,
     SUM(C.ENTRADA)- SUM(C.SALIDA) AS SALDO ,
     C.SECTOR,
     S.NOMBRE_SECTOR
FROM
    tbsectores AS S INNER JOIN tbcaja AS C ON S.ID = C.SECTOR
     where C.FECHA_MOVIMIENTO  between $P{desde} and  $P{hasta}
GROUP BY C.SECTOR ORDER BY C.SECTOR]]>
	</queryString>
